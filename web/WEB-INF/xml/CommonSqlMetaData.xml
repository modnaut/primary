<?xml version="1.0" encoding="UTF-8"?>
<sqlMetaData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/SqlMetaData.xsd">
	<query name="GET_USER_LIST" action="SELECT" type="QUERY">
		<statement value="Select UserName, FirstName, LastName, EmailAddress From Users Where UserId = ?"/>
		<parameters>
			<parameter name="UserId" type="INT"/>
		</parameters>
	</query>
	<query name="GET_ALL_USERS_ALPHABETICALLY" action="SELECT" type="SP">
		<statement value="Common.up_GetAllUsersAlphabetically()"/>
	</query>
	<query name="INSERT_USER" action="ACTION" type="QUERY">
		<statement value="INSERT INTO users (UserName, FirstName, LastName, EmailAddress) 
						  VALUES (?, ?, ?, ?)"/>
		<parameters>
			<parameter name="UserName" type="VARCHAR"/>
			<parameter name="FirstName" type="VARCHAR"/>
			<parameter name="LastName" type="VARCHAR"/>
			<parameter name="EmailAddress" type="VARCHAR"/>
		</parameters>
	</query>
	<query name="AUTHENTICATE_USER" action="SELECT" type="SP">
		<statement value="Common.up_AuthenticateUser(?, ?)"/>
		<parameters>
			<parameter name="Email" type="VARCHAR"/>
			<parameter name="Password" type="VARCHAR"/>
		</parameters>
	</query>
	<query name="INSERT_UPDATE_SESSION" action="ACTION" type="SP">
		<statement value="Common.up_InsertUpdateSession(?, ?, ?)"/>
		<parameters>
			<parameter name="UserId" type="INT"/>
			<parameter name="SessionId" type="INT"/>
			<parameter name="SessionObject" type="BLOB"/>
		</parameters>
	</query>
	<query name="GET_SESSION" action="SELECT" type="QUERY">
		<statement value="SELECT SessionObject, LastModifiedDate FROM Common.Session WHERE SessionId = ?"/>
		<parameters>
			<parameter name="SessionId" type="INT"/>
		</parameters>
	</query>
	<query name="GET_USER_SESSION" action="SELECT" type="QUERY">
		<statement value="Common.up_GetUserSession(?)"/>
		<parameters>
			<parameter name="UserId" type="INT"/>
		</parameters>
	</query>
	<query name="GET_USER" action="SELECT" type="SP">
		<statement value="Common.up_GetUser(?)"/>
		<parameters>
			<parameter name="UserId" type="INT"/>
		</parameters>
	</query>
	<query name="GET_USER_TYPES" action="SELECT" type="QUERY">
		<statement value="Select UserTypeCd, Description From Common.UserType Order By Description ASC"/>
	</query>
	<query name="UPDATE_USER" action="ACTION" type="SP">
		<statement value="Common.up_UpdateUser(?, ?, ?, ?, ?, ?, ?, ?)"/>
		<parameters>
			<parameter name="UserId" type="INT"/>
			<parameter name="UserName" type="VARCHAR"/>
			<parameter name="FirstName" type="VARCHAR"/>
			<parameter name="LastName" type="VARCHAR"/>
			<parameter name="EmailAddress" type="VARCHAR"/>
			<parameter name="HireDateDisabled" type="VARCHAR"/>
			<parameter name="UserTypeCd" type="VARCHAR"/>
			<parameter name="UserPassword" type="VARCHAR"/>
		</parameters>
	</query>
	<query name="GET_TRANSLATED_STRINGS" action="SELECT" type="SP">
		<statement value="Common.up_GetTranslatedStrings(?, ?)"/>
		<parameters>
			<parameter name="stringCds" type="VARCHAR"/>
			<parameter name="languageCd" type="VARCHAR"/>
		</parameters>
	</query>
</sqlMetaData>