<?xml version="1.0" encoding="UTF-8"?>
<viewMetaData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../xsd/viewmetadata.xsd">
	<items>
		<item xsi:type="TabPanel" width="100" height="50" id="top" titleAlign="center" tabPosition="bottom" plain="true" activeTab="1">
			<icon path="silk/wrench.png"/>
			<items>
				<item xsi:type="Panel">
					<items>
						<item xsi:type="Button">
							<text stringCD="button Text"/>
							<tooltip stringCD="button tool tip"/>
						</item>
						<item xsi:type="CheckboxGroup" allowBlank="false" columns="[1,2,3]" disabled="true">
							<blankText stringCD="select 1 damnit!"/>
							<fieldLabel stringCD="checkbox group"/>
							<items>
								<item xsi:type="Checkbox" checked="true">
									<boxLabel stringCD="box 1"/>
								</item>
								<item xsi:type="Checkbox" checked="true">
									<boxLabel stringCD="box 2"/>
								</item>
							</items>
						</item>
						<item xsi:type="Radio" radioGroupName="size" checked="true">
							<boxLabel stringCD="radio 1"/>
						</item>
						<item xsi:type="Radio" radioGroupName="size" value="true" inputValue="second one">
							<boxLabel stringCD="label 2"/>
						</item>
					</items>
					<title stringCD="tab 1"/>
				</item>
				<item xsi:type="GridPanel" id="dannytab">
					<title stringCD="tab 2"/>
					<column dataIndex="userId">
						<text stringCD="User ID"/>
					</column>
					<column dataIndex="userName">
						<text stringCD="Username"/>
					</column>
					<column dataIndex="email">
						<text stringCD="Email Address"/>
					</column>
					<column dataIndex="firstName">
						<text stringCD="First Name"/>
					</column>
					<column dataIndex="lastName">
						<text stringCD="Last Name"/>
					</column>
					<store xsi:type="Store">
						<model>
							<field name="userId" type="int"/>
							<field name="userName" type="string"/>
							<field name="email" type="string"/>
							<field name="firstName" type="string"/>
							<field name="lastName" type="string"/>
						</model>
						<data>
							<record>
								<field name="userId" dataIndex="0"/>
								<field name="userName" dataIndex="1"/>
								<field name="email" dataIndex="2"/>
								<field name="firstName" dataIndex="3"/>
								<field name="lastName" dataIndex="4"/>
							</record>
						</data>
					</store>
				</item>
			</items>
			<title stringCD="danny"/>
		</item>
	</items>
</viewMetaData>

