						<item xsi:type="Radio" radioGroupName="size" checked="true">
							<boxLabel stringCD="radio 1"/>
						</item>
						<item xsi:type="Radio" radioGroupName="size" value="true" inputValue="second one">
							<boxLabel stringCD="label 2"/>
						</item>
