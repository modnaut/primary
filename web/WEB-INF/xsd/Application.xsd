<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:str="String">
	<xs:import schemaLocation="String.xsd" namespace="String"/>
	<xs:element name="applications">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="application" type="Application" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Application">
		<xs:sequence>
			<xs:element name="pageTitle" type="str:String" minOccurs="1" maxOccurs="1"/>
			<xs:element name="applicationFolder" type="xs:string" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="applicationId" type="xs:int" use="required"/>
	</xs:complexType>
</xs:schema>
