<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:str="String" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="String" schemaLocation="String.xsd"/>
	<xs:element name="viewMetaData">
		<xs:complexType>
			<xs:choice>
				<xs:element name="viewport" type="Viewport"/>
				<xs:element name="items" type="ItemSet"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Viewport">
		<xs:sequence>
			<xs:element name="item" type="Item" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ItemSet">
		<xs:sequence>
			<xs:element name="item" type="Item" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Item" abstract="true">
		<xs:attribute name="id" type="xs:string"/>
		<xs:attribute name="width" type="xs:int"/>
		<xs:attribute name="height" type="xs:int"/>
	</xs:complexType>
	<xs:complexType name="AbstractLayout" abstract="true">
		<xs:attribute name="itemCls" type="xs:string"/>
		<xs:attribute name="reserveScrollbar" type="xs:boolean" default="false"/>
	</xs:complexType>
	<xs:complexType name="AutoLayout">
		<xs:complexContent>
			<xs:extension base="AbstractLayout"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AnchorLayout">
		<xs:complexContent>
			<xs:extension base="AbstractLayout">
				<xs:attribute name="defaultAnchor" type="xs:string" default="100%"/>
				<xs:attribute name="manageOverflow" type="xs:int" default="2"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbsoluteLayout">
		<xs:complexContent>
			<xs:extension base="AnchorLayout">
				<xs:attribute name="ignoreOnContentChange" type="xs:boolean" default="true"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="LayoutPack">
		<xs:restriction base="xs:string">
			<xs:enumeration value="start"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="end"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="HBoxLayoutAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="top"/>
			<xs:enumeration value="middle"/>
			<xs:enumeration value="bottom"/>
			<xs:enumeration value="stretch"/>
			<xs:enumeration value="stretchmax"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VBoxLayoutAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="right"/>
			<xs:enumeration value="stretch"/>
			<xs:enumeration value="stretchmax"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BoxLayout" abstract="true">
		<xs:complexContent>
			<xs:extension base="AbstractLayout">
				<xs:attribute name="constrainAlign" type="xs:boolean" default="false"/>
				<xs:attribute name="pack" type="LayoutPack" default="start"/>
				<xs:attribute name="padding" type="xs:string" default="0"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HBoxLayout">
		<xs:complexContent>
			<xs:extension base="BoxLayout">
				<xs:attribute name="align" type="HBoxLayoutAlign" default="top"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VBoxLayout">
		<xs:complexContent>
			<xs:extension base="BoxLayout">
				<xs:attribute name="align" type="VBoxLayoutAlign" default="left"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AccordionLayout">
		<xs:complexContent>
			<xs:extension base="VBoxLayout">
				<xs:attribute name="activeOnTop" type="xs:boolean" default="false"/>
				<xs:attribute name="animate" type="xs:boolean" default="true"/>
				<xs:attribute name="collapseFirst" type="xs:boolean" default="true"/>
				<xs:attribute name="fill" type="xs:boolean" default="true"/>
				<xs:attribute name="hideCollapseTool" type="xs:boolean" default="false"/>
				<xs:attribute name="multi" type="xs:boolean" default="false"/>
				<xs:attribute name="titleCollapse" type="xs:boolean" default="true"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ColumnLayout">
		<xs:complexContent>
			<xs:extension base="AbstractLayout"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FitLayout">
		<xs:complexContent>
			<xs:extension base="AbstractLayout"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CenterLayout">
		<xs:complexContent>
			<xs:extension base="FitLayout"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Listener"/>
	<xs:complexType name="Menu"/>
	<xs:complexType name="ImageSpec">
		<xs:attribute name="path" type="xs:string" use="required"/>
		<xs:attribute name="external" type="xs:boolean" default="false"/>
	</xs:complexType>
	<xs:simpleType name="Region">
		<xs:restriction base="xs:string">
			<xs:enumeration value="north"/>
			<xs:enumeration value="south"/>
			<xs:enumeration value="east"/>
			<xs:enumeration value="west"/>
			<xs:enumeration value="center"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Container">
		<xs:complexContent>
			<xs:extension base="Item">
				<xs:attribute name="autoScroll" type="xs:boolean" default="false"/>
				<xs:attribute name="border" type="xs:string"/>
				<xs:attribute name="cls" type="xs:string"/>
				<xs:attribute name="columnWidth" type="xs:string"/>
				<xs:attribute name="html" type="xs:string"/>
				<xs:attribute name="margin" type="xs:string"/>
				<xs:attribute name="minHeight" type="xs:int"/>
				<xs:attribute name="maxHeight" type="xs:int"/>
				<xs:attribute name="minWidth" type="xs:int"/>
				<xs:attribute name="maxWidth" type="xs:int"/>
				<xs:attribute name="padding" type="xs:string"/>
				<xs:attribute name="region" type="Region"/>
				<xs:attribute name="resizable" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>
							For containers resizable defualts to false,
							but for grid columns it defaults to true.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="shrinkWrap" type="xs:int" default="2"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="CollapseDirection">
		<xs:restriction base="xs:string">
			<xs:enumeration value="top"/>
			<xs:enumeration value="bottom"/>
			<xs:enumeration value="left"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CollapseMode">
		<xs:restriction base="xs:string">
			<xs:enumeration value="header"/>
			<xs:enumeration value="mini"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="HeaderPosition">
		<xs:restriction base="xs:string">
			<xs:enumeration value="top"/>
			<xs:enumeration value="bottom"/>
			<xs:enumeration value="left"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TitleAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Panel">
		<xs:complexContent>
			<xs:extension base="Container">
				<xs:sequence>
					<xs:element name="icon" type="ImageSpec" minOccurs="0" maxOccurs="1"/>
					<xs:element name="items" type="ItemSet" minOccurs="0" maxOccurs="1"/>
					<xs:element name="layout" type="AbstractLayout" minOccurs="0" maxOccurs="1"/>
					<xs:element name="listener" type="Listener" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="title" type="str:String" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
				<xs:attribute name="animCollapse" type="xs:boolean" default="true"/>
				<xs:attribute name="bodyPadding" type="xs:string"/>
				<xs:attribute name="collapseDirection" type="CollapseDirection"/>
				<xs:attribute name="collapseFirst" type="xs:boolean" default="true"/>
				<xs:attribute name="collapseMode" type="CollapseMode"/>
				<xs:attribute name="collapsed" type="xs:boolean" default="false"/>
				<xs:attribute name="collapsible" type="xs:boolean" default="false"/>
				<xs:attribute name="headerPosition" type="HeaderPosition" default="top"/>
				<xs:attribute name="hideCollapseTool" type="xs:boolean" default="false"/>
				<xs:attribute name="hideHeader" type="xs:boolean" default="false"/>
				<xs:attribute name="iconCls" type="xs:string"/>
				<xs:attribute name="titleAlign" type="TitleAlign" default="left"/>
				<xs:attribute name="titleCollapse" type="xs:boolean" default="false"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="Align">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Column">
		<xs:complexContent>
			<xs:extension base="Container">
				<xs:sequence>
					<xs:element name="column" type="Column" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="emptyCellText" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="menuText" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="text" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="tooltip" type="str:String" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
				<xs:attribute name="align" type="Align" default="left"/>
				<xs:attribute name="dataIndex" type="xs:string"/>
				<xs:attribute name="draggable" type="xs:boolean" default="true"/>
				<xs:attribute name="groupable" type="xs:boolean" default="true"/>
				<xs:attribute name="hideable" type="xs:boolean" default="true"/>
				<xs:attribute name="lockable" type="xs:boolean" default="true"/>
				<xs:attribute name="locked" type="xs:boolean" default="false"/>
				<xs:attribute name="menuDisabled" type="xs:boolean" default="false"/>
				<xs:attribute name="sealed" type="xs:boolean" default="false"/>
				<xs:attribute name="sortable" type="xs:boolean" default="true"/>
				<xs:attribute name="tdCls" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="Scroll">
		<xs:restriction base="xs:string">
			<xs:enumeration value="none"/>
			<xs:enumeration value="both"/>
			<xs:enumeration value="horizontal"/>
			<xs:enumeration value="vertical"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="TablePanel" abstract="true">
		<xs:complexContent>
			<xs:extension base="Panel">
				<xs:sequence>
					<xs:element name="column" type="Column" minOccurs="1" maxOccurs="unbounded"/>
					<xs:element name="emptyText" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="store" type="AbstractStore" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
				<xs:attribute name="allowDeselect" type="xs:boolean" default="false"/>
				<xs:attribute name="columnLines" type="xs:boolean" default="false"/>
				<xs:attribute name="disableSelection" type="xs:boolean" default="false"/>
				<xs:attribute name="enableColumnHide" type="xs:boolean" default="true"/>
				<xs:attribute name="enableColumnMove" type="xs:boolean" default="true"/>
				<xs:attribute name="enableColumnResize" type="xs:boolean" default="true"/>
				<xs:attribute name="enableLocking" type="xs:boolean" default="false"/>
				<xs:attribute name="hideHeaders" type="xs:boolean" default="false"/>
				<xs:attribute name="rowLines" type="xs:boolean" default="true"/>
				<xs:attribute name="scroll" type="Scroll" default="both"/>
				<xs:attribute name="sortableColumns" type="xs:boolean" default="true"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GridPanel">
		<xs:complexContent>
			<xs:extension base="TablePanel"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="TabPosition">
		<xs:restriction base="xs:string">
			<xs:enumeration value="top"/>
			<xs:enumeration value="bottom"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="TabPanel">
		<xs:complexContent>
			<xs:extension base="Panel">
				<xs:attribute name="activeTab" type="xs:string" default="0"/>
				<xs:attribute name="deferredRender" type="xs:boolean" default="true"/>
				<xs:attribute name="minTabWidth" type="xs:int"/>
				<xs:attribute name="maxTabWidth" type="xs:int"/>
				<xs:attribute name="plain" type="xs:boolean" default="false"/>
				<xs:attribute name="removePanelHeader" type="xs:boolean" default="true"/>
				<xs:attribute name="tabPosition" type="TabPosition" default="top"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:attributeGroup name="Labelable">
		<xs:attribute name="autoFitErrors" type="xs:boolean" default="true"/>
		<xs:attribute name="hideEmptyLabel" type="xs:boolean" default="true"/>
		<xs:attribute name="hideLabel" type="xs:boolean" default="false"/>
		<xs:attribute name="labelAlign" type="LabelAlign" default="left"/>
		<xs:attribute name="labelPad" type="xs:int" default="5"/>
		<xs:attribute name="labelSeparator" type="xs:string" default=":"/>
		<xs:attribute name="labelWidth" type="xs:int" default="100"/>
		<xs:attribute name="msgTarget" type="MsgTarget" default="qtip"/>
		<xs:attribute name="preventMark" type="xs:boolean" default="false"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="Field">
		<xs:attribute name="disabled" type="xs:boolean" default="false"/>
		<xs:attribute name="submitValue" type="xs:boolean" default="true"/>
		<xs:attribute name="validateOnChange" type="xs:boolean" default="true"/>
		<xs:attribute name="value" type="xs:string"/>
	</xs:attributeGroup>
	<xs:complexType name="CheckboxGroup">
		<xs:complexContent>
			<xs:extension base="Container">
				<xs:sequence>
					<xs:element name="activeError" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="blankText" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="fieldLabel" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="items" type="ItemSet" minOccurs="0" maxOccurs="1"/>
					<xs:element name="layout" type="AbstractLayout" minOccurs="0" maxOccurs="1"/>
					<xs:element name="listener" type="Listener" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="title" type="str:String" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
				<xs:attributeGroup ref="Field"/>
				<xs:attributeGroup ref="Labelable"/>
				<xs:attribute name="allowBlank" type="xs:boolean" default="true"/>
				<xs:attribute name="columns" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="ArrowAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="right"/>
			<xs:enumeration value="bottom"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IconAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="top"/>
			<xs:enumeration value="right"/>
			<xs:enumeration value="bottom"/>
			<xs:enumeration value="left"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ButtonScale">
		<xs:restriction base="xs:string">
			<xs:enumeration value="small"/>
			<xs:enumeration value="medium"/>
			<xs:enumeration value="large"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TextAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="center"/>
			<xs:enumeration value="left"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Button">
		<xs:complexContent>
			<xs:extension base="Item">
				<xs:sequence>
					<xs:element name="icon" type="ImageSpec" minOccurs="0" maxOccurs="1"/>
					<xs:element name="listener" type="Listener" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="menu" type="Menu" minOccurs="0" maxOccurs="1"/>
					<xs:element name="text" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="tooltip" type="str:String" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
				<xs:attribute name="allowDepress" type="xs:boolean" default="true"/>
				<xs:attribute name="arrowAlign" type="ArrowAlign" default="right"/>
				<xs:attribute name="border" type="xs:string"/>
				<xs:attribute name="disabled" type="xs:boolean" default="false"/>
				<xs:attribute name="enableToggle" type="xs:boolean" default="false"/>
				<xs:attribute name="handleMouseEvents" type="xs:boolean" default="true"/>
				<xs:attribute name="iconAlign" type="IconAlign" default="left"/>
				<xs:attribute name="iconCls" type="xs:string"/>
				<xs:attribute name="menuAlign" type="xs:string" default="tl-bl?">
					<xs:annotation>
						<xs:documentation>
							See Ext.Element.alignTo
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="overflowText" type="xs:string"/>
				<xs:attribute name="pressed" type="xs:boolean" default="false"/>
				<xs:attribute name="scale" type="ButtonScale" default="small"/>
				<xs:attribute name="shrinkWrap" type="xs:int" default="3"/>
				<xs:attribute name="tabIndex" type="xs:int"/>
				<xs:attribute name="textAlign" type="TextAlign" default="center"/>
				<xs:attribute name="toggleGroup" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="LabelAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="top"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MsgTarget">
		<xs:restriction base="xs:string">
			<xs:enumeration value="qtip"/>
			<xs:enumeration value="title"/>
			<xs:enumeration value="under"/>
			<xs:enumeration value="side"/>
			<xs:enumeration value="none"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AbstractField" abstract="true">
		<xs:complexContent>
			<xs:extension base="Item">
				<xs:sequence>
					<xs:element name="activeError" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="fieldLabel" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="invalidText" type="str:String" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
				<xs:attributeGroup ref="Field"/>
				<xs:attributeGroup ref="Labelable"/>
				<xs:attribute name="readOnly" type="xs:boolean" default="false"/>
				<xs:attribute name="tabIndex" type="xs:int"/>
				<xs:attribute name="validateOnBlur" type="xs:boolean" default="true"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="BoxLabelAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="before"/>
			<xs:enumeration value="after"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Checkbox">
		<xs:complexContent>
			<xs:extension base="AbstractField">
				<xs:sequence>
					<xs:element name="boxLabel" type="str:String" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
				<xs:attribute name="boxLabelAlign" type="BoxLabelAlign" default="after"/>
				<xs:attribute name="checked" type="xs:boolean" default="false"/>
				<xs:attribute name="inputValue" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Radio">
		<xs:complexContent>
			<xs:extension base="Checkbox">
				<xs:attribute name="radioGroupName" type="xs:string" use="required"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DisplayField">
		<xs:complexContent>
			<xs:extension base="AbstractField"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HiddenField">
		<xs:complexContent>
			<xs:extension base="AbstractField"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="VType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="alpha"/>
			<xs:enumeration value="alphanum"/>
			<xs:enumeration value="email"/>
			<xs:enumeration value="url"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="TextField">
		<xs:complexContent>
			<xs:extension base="AbstractField">
				<xs:sequence>
					<xs:element name="blankText" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="emptyText" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="maxLengthText" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="minLengthText" type="str:String" minOccurs="0" maxOccurs="1"/>
					<xs:element name="regexText" type="str:String" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
				<xs:attribute name="allowBlank" type="xs:boolean" default="true"/>
				<xs:attribute name="allowOnlyWhitespace" type="xs:boolean" default="true"/>
				<xs:attribute name="disableKeyFilter" type="xs:boolean" default="false"/>
				<xs:attribute name="enforceMaxLength" type="xs:boolean" default="false"/>
				<xs:attribute name="grow" type="xs:boolean" default="false"/>
				<xs:attribute name="growMax" type="xs:int" default="800"/>
				<xs:attribute name="growMin" type="xs:int" default="30"/>
				<xs:attribute name="maxLength" type="xs:int"/>
				<xs:attribute name="minLength" type="xs:int"/>
				<xs:attribute name="regex" type="xs:string"/>
				<xs:attribute name="selectOnFocus" type="xs:boolean" default="false"/>
				<xs:attribute name="vtype" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TextArea">
		<xs:complexContent>
			<xs:extension base="TextField">
				<xs:attribute name="cols" type="xs:int" default="20"/>
				<xs:attribute name="enterIsSpecial" type="xs:boolean" default="false"/>
				<xs:attribute name="preventScrollbars" type="xs:boolean" default="false"/>
				<xs:attribute name="rows" type="xs:int" default="4"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TriggerField" abstract="true">
		<xs:complexContent>
			<xs:extension base="AbstractField">
				<xs:attribute name="editable" type="xs:boolean" default="true"/>
				<xs:attribute name="hideTrigger" type="xs:boolean" default="false"/>
				<xs:attribute name="selectOnFocus" type="xs:boolean" default="false"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FileField">
		<xs:complexContent>
			<xs:extension base="TriggerField">
				<xs:sequence>
